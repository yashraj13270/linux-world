import smtplib
import ssl
from email.message import EmailMessage

EMAIL = "tec"  # Replace with your actual email
APP_PASSWORD = "NNNJ"  # Replace with your actual app password
RECEIVER = "HUHBYG"  # Replace with actual receiver email

msg = EmailMessage()
msg["From"] = EMAIL
msg["To"] = RECEIVER
msg["Subject"] = "Hello For Python ...."
msg.set_content("This email was shared by python code...")

context = ssl.create_default_context()
with smtplib.SMTP_SSL("smtp.gmail.com", 465, context=context) as server:  # Fixed typo in "smtp.gmail.com"
    server.login(EMAIL, APP_PASSWORD)
    server.send_message(msg)  # Fixed from SERVER.send.message to server.send_message
