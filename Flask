from flask import Flask
app = Flask(__name__)
@app.route("/")
def home():
    return """
    <h1> I AM AT HOME PAGE</h1>
    <h2> i am running flask</h2>
    """
@app.route("/about")
def about():
    return """
    <h1> I am about page </h1>
    <h1> I am at page </h2>



    
    """
if __name__=='__main__':
    app.run()
